
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}
model User {
  id          String  @id @default(auto()) @map("_id") @db.ObjectId
  email       String
  password String
}

model staff{
  id          String  @id @default(auto()) @map("_id") @db.ObjectId
  usertname String @unique
  name       String
  email       String
  password String
}